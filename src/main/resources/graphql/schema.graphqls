type OrderSummary {
    order: Order
    shipment: Shipment
    returnInfo: ReturnInfo
}

type Order {
    id: ID!
    info: String
    shipmentID: Int
    returnID: Int
}

type Shipment {
    id: ID!
    address: String
    date: String
}

type ReturnInfo {
    id: ID!
    accepted: Boolean
    reason: String
}

type Query {
    orderSummary(id: ID): OrderSummary
}
